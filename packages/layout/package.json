{
  "name": "@knockout-js/layout",
  "version": "0.1.19",
  "description": "组件功能描述",
  "files": [
    "esm",
    "es2017",
    "cjs",
    "dist"
  ],
  "main": "esm/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "exports": {
    ".": {
      "es2017": {
        "types": "./es2017/index.d.ts",
        "default": "./es2017/index.js"
      },
      "default": {
        "types": "./esm/index.d.ts",
        "default": "./esm/index.js"
      }
    },
    "./*": "./*"
  },
  "sideEffects": [
    "dist/*",
    "*.scss",
    "*.less",
    "*.css"
  ],
  "scripts": {
    "start": "ice-pkg start",
    "build": "rimraf esm es2017 && ice-pkg build",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ice",
    "react",
    "component"
  ],
  "dependencies": {
    "@ant-design/icons": "^5.2.5",
    "@ant-design/pro-components": "^2.6.28",
    "@dnd-kit/core": "^6.0.8",
    "@dnd-kit/sortable": "^7.0.2",
    "@dnd-kit/utilities": "^3.2.1",
    "@ice/jsx-runtime": "^0.2.0",
    "@ice/runtime": "^1.2.4",
    "@knockout-js/api": "workspace:^",
    "@knockout-js/ice-urql": "workspace:^",
    "@swc/helpers": "^0.5.1",
    "antd": "^5.6.3",
    "dayjs": "^1.11.9",
    "react-activation": "^0.12.4",
    "react-draggable": "^4.4.5"
  },
  "devDependencies": {
    "@ice/pkg": "^1.5.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "react": "^17 || ^18"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "http",
    "url": "https://github.com/woocoos/knockout-js/tree/main/packages/layout"
  },
  "license": "MIT"
}
