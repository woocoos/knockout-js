{
  "name": "@knockout-js/org",
  "version": "0.0.4-beta.1",
  "description": "Knockout组织相关组件",
  "files": [
    "esm",
    "cjs",
    "es2017",
    "dist"
  ],
  "main": "esm/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "exports": {
    ".": {
      "es2017": {
        "types": "./es2017/index.d.ts",
        "default": "./es2017/index.js"
      },
      "default": {
        "types": "./esm/index.d.ts",
        "default": "./esm/index.js"
      }
    },
    "./*": "./*"
  },
  "sideEffects": [
    "dist/*",
    "*.scss",
    "*.less",
    "*.css"
  ],
  "scripts": {
    "start": "ice-pkg start",
    "build": "rimraf esm es2017 && ice-pkg build",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "icepkg",
    "react",
    "component"
  ],
  "dependencies": {
    "@ant-design/icons": "^5.2.5",
    "@ant-design/pro-table": "^3.8.2",
    "@ice/jsx-runtime": "^0.2.0",
    "@knockout-js/api": "workspace:^",
    "@knockout-js/layout": "workspace:^",
    "@knockout-js/ice-urql": "workspace:^",
    "@swc/helpers": "^0.5.1",
    "antd": "^5.6.3"
  },
  "devDependencies": {
    "@ice/pkg": "^1.5.11",
    "@types/react": "^18.0.0",
    "react": "^18.0.0",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "react": "^17 || ^18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT"
}
