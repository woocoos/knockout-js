{
  "name": "@knockout-js/ice-urql",
  "version": "0.1.3",
  "description": "urql plugin for ice",
  "license": "MIT",
  "type": "module",
  "main": "./es2017/index.js",
  "types": "./es2017/index.d.ts",
  "exports": {
    ".": {
      "types": "./es2017/index.d.ts",
      "import": "./es2017/index.js",
      "default": "./es2017/index.js"
    },
    "./runtime": {
      "types": "./es2017/runtime.d.ts",
      "import": "./es2017/runtime.js",
      "default": "./es2017/runtime.js"
    },
    "./request": {
      "types": "./es2017/request.d.ts",
      "import": "./es2017/request.js",
      "default": "./es2017/request.js"
    },
    "./types": {
      "types": "./es2017/types.d.ts",
      "import": "./es2017/types.js",
      "default": "./es2017/types.js"
    },
    "./*": "./*"
  },
  "files": [
    "es2017",
    "!es2017/**/*.map",
    "*.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "http",
    "url": "https://github.com/woocoos/knockout-js/tree/main/packages/ice-urql"
  },
  "scripts": {
    "start": "ice-pkg start",
    "build": "rimraf esm es2017 && ice-pkg build",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@ice/app": "^3.2.6",
    "@ice/jsx-runtime": "^0.2.0",
    "@ice/plugin-request": "^1.0.1",
    "@ice/runtime": "^1.2.4",
    "@swc/helpers": "^0.5.1",
    "@urql/exchange-auth": "^2.1.6",
    "antd": "^5.6.3",
    "crypto-js": "^4.1.1",
    "graphql-ws": "^5.14.0",
    "jwt-decode": "^3.1.2",
    "urql": "^4.0.5"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/react": "^18.0.0",
    "axios": "^1.4.0",
    "react": "^18.0.0",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
