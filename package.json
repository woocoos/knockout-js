{
  "name": "knockout-js",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "pnpm packages:start & pnpm docs:start",
    "build": "pnpm packages:build && pnpm docs:build",
    "packages:start": "pnpm -r run start",
    "packages:build": "pnpm -r run build",
    "docs:start": "ice-pkg start",
    "docs:build": "ice-pkg build",
    "eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx ./",
    "eslint:fix": "pnpm eslint --fix",
    "stylelint": "stylelint \"**/*.{css,scss,less}\"",
    "stylelint:fix": "pnpm stylelint --fix",
    "lint": "pnpm eslint && pnpm stylelint",
    "lint:fix": "pnpm eslint:fix && pnpm stylelint:fix",
    "test": "vitest",
    "ci:publish": "pnpm publish -r"
  },
  "devDependencies": {
    "@ant-design/icons": "^5.1.4",
    "@applint/spec": "^1.0.0",
    "@changesets/cli": "^2.26.2",
    "@docusaurus/module-type-aliases": "^2.4.1",
    "@docusaurus/theme-classic": "^2.4.1",
    "@ice/pkg": "^1.5.11",
    "@ice/pkg-plugin-docusaurus": "^1.4.15",
    "@ice/remark-react-docgen-docusaurus": "^1.0.1",
    "@knockout-js/api": "workspace:*",
    "@knockout-js/layout": "workspace:*",
    "@knockout-js/org": "workspace:*",
    "@knockout-js/ice-urql": "workspace:*",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@tsconfig/docusaurus": "^2.0.0",
    "antd": "^5.6.3",
    "clsx": "^1.2.1",
    "eslint": "^8.0.0",
    "jsdom": "^22.1.0",
    "msw": "^1.2.3",
    "react": "^18.2.0",
    "react-activation": "^0.12.4",
    "react-dom": "^18.2.0",
    "rimraf": "^4.3.1",
    "stylelint": "^15.0.0",
    "urql": "^4.0.4",
    "vitest": "^0.33.0"
  },
  "packageManager": "pnpm@7.22.0"
}
